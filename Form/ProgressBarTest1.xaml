<Window x:Class="CreatePipe.Form.ProgressBarTest1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CreatePipe.Form"
        mc:Ignorable="d"
        Title="ProgressBarTest1" Height="450" Width="800">
    <Window.TaskbarItemInfo>
        <TaskbarItemInfo ProgressState="Normal"/>
    </Window.TaskbarItemInfo>
    <ProgressBar Height="10" Width="300" Maximum="100"
                 MouseLeftButtonDown="ProgressBar_MouseLeftButtonDown"/>
</Window>
